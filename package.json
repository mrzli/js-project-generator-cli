{
  "name": "@gmjs/js-project-generator-cli",
  "version": "0.0.16",
  "description": "CLI for JavaScript/TypeScript project generator.",
  "author": {
    "name": "Goran MrÅ¾ljak",
    "email": "goran.mrzljak@gmail.com",
