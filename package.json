  "repository": {
    "type": "git",
    "url": "https://github.com/mrzli/js-project-generator-cli"
  },
  "homepage": "https://github.com/mrzli/js-project-generator-cli",
  "main": "./src/index.js",
  "bin": {
    "jsgen": "src/index.js"
  },
  "scripts": {
    "dev": "ts-node src/index.ts",
    "lint": "eslint --fix . && prettier --write .",
    "lint:nofix": "eslint . && prettier --check .",
    "test-only": "jest --passWithNoTests",
    "test": "pnpm run lint && pnpm run test-only",
    "clean": "shx rm -rf ./dist",
    "build-only": "pnpm run clean && tsc --project tsconfig.lib.json",
    "build": "pnpm run test && pnpm run build-only",
    "pac-only": "pnpmpub pack",
    "pac": "pnpm run build && pnpm run pac-only",
    "pub-only": "pnpmpub pub",
    "pub": "pnpm run pac && pnpm run pub-only"
  },
  "dependencies": {
    "@gmjs/data-container-util": "^0.0.1",
    "@gmjs/fs-async": "^0.0.5",
    "@gmjs/js-project-generator": "^0.0.24",
    "@gmjs/package-json": "^0.0.2",
    "@inquirer/prompts": "^5.0.7",
    "commander": "^12.1.0",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@gmjs/eslint-config": "^0.0.37",
    "@gmjs/pnpm-publish-cli": "^0.0.2",
    "@jest/globals": "^29.7.0",
    "@types/node": "^20.14.9",
    "eslint": "^9.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "type-fest": "^4.20.1",
    "typescript": "^5.5.2"
  },
  "engines": {
    "node": ">=22.3.0"
  }
}
